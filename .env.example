# Database
MEMORY_DB_PATH=bot_memory.db
EMBEDDING_MODEL=your_embedding_model_here

# Scheduler Settings
ENABLE_SCHEDULE=true
MORNING_TIME=08:00
EVENING_TIME=17:00
TIMEZONE=Europe/Moscow
